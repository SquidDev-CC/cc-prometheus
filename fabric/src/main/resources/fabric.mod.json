{
    "schemaVersion": 1,
    "id": "ccprometheus",
    "version": "${version}",
    "name": "CC Prometheus Exporter",
    "description": "Hosts a Prometheus server with various ComputerCraft related statistics",
    "authors": [
        "SquidDev"
    ],
    "contact": {
        "homepage": "https://github.com/cc-tweaked/cc-prometheus",
        "sources": "https://github.com/cc-tweaked/cc-prometheus"
    },
